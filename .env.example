# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your_minio_password_here
MINIO_DOMAIN=minio
MINIO_CONSOLE_PORT=9001
MINIO_API_PORT=9000

# PostgreSQL Configuration
POSTGRES_USER=admin
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DB=metastore
POSTGRES_PORT=5432

# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW_USER=airflow
AIRFLOW_PASSWORD=your_airflow_password_here
AIRFLOW_FIRSTNAME=Airflow
AIRFLOW_LASTNAME=Admin
AIRFLOW_EMAIL=airflow@example.com

# Superset Configuration
SUPERSET_SECRET_KEY=your_secret_key_here_min_32_chars
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=your_superset_password_here
SUPERSET_ADMIN_EMAIL=admin@example.com
SUPERSET_LOAD_EXAMPLES=no

# Redis Configuration (para Superset cache)
REDIS_HOST=redis
REDIS_PORT=6379

# Trino Configuration
TRINO_ENVIRONMENT=production

# Spark Configuration
SPARK_MASTER_PORT=7077
SPARK_MASTER_WEBUI_PORT=8080
SPARK_WORKER_WEBUI_PORT=8081

# Hive Metastore
HIVE_METASTORE_PORT=9083

# Network
NETWORK_NAME=bigdata-network
